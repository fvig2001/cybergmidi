<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!-- <Color x:Key="Panel1Color">#474747</Color> -->
    <SolidColorBrush x:Key="Panel1Color">#474747</SolidColorBrush>
    <SolidColorBrush x:Key="Panel2Color">#7F7F7F</SolidColorBrush>
    <SolidColorBrush x:Key="Panel3Color">#C3C3C3</SolidColorBrush>
    <SolidColorBrush x:Key="StatusBarColor">#7F7F7F</SolidColorBrush>
    <SolidColorBrush  x:Key="BorderColor">#FFFFFFFF</SolidColorBrush>
    <SolidColorBrush  x:Key="STATUSNG">#FF0000</SolidColorBrush>
    <SolidColorBrush  x:Key="STATUSOK">#00FF00</SolidColorBrush>
    <Style TargetType="ComboBox" BasedOn="{StaticResource {x:Type ComboBox}}">
        <Style.Triggers>
            <Trigger Property="IsEnabled" Value="False">
                <Setter Property="Opacity" Value="0.4"/>
                <Setter Property="Foreground" Value="DarkRed"/>
                <Setter Property="Background" Value="LightGray"/>
                <Setter Property="Cursor" Value="No" />
            </Trigger>
        </Style.Triggers>
    </Style>
</ResourceDictionary>